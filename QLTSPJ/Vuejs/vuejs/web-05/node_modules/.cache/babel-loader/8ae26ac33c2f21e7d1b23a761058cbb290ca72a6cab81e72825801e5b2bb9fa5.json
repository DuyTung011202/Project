{"ast":null,"code":"import { Form } from \"../../../js/common/form.js\";\nimport { DateConfig } from \"../../../js/common/config.js\";\nimport MISAFunction from \"../../../js/common/function\";\nimport { departmentURL } from \"@/js/common/urlAsset\";\nimport axios from \"axios\";\nexport default {\n  name: \"MISAForm\",\n  props: {\n    isVisable: {\n      type: Boolean\n    },\n    productEdit: {\n      type: Object\n    }\n  },\n  created() {\n    this.product = this.productEdit;\n    if (this.formMode == this.$_MISAEnum.FORM_MODE.ADD) {\n      // Lẫy mã nhân viên mới:\n      axios.get(\"https://cukcuk.manhnv.net/api/v1/Customers/NewCustomerCode\").then(res => {\n        this.product.ProductsCode = res.data;\n      }).catch(res => {\n        console.log();\n        res;\n      });\n    }\n  },\n  watch: {\n    product: {\n      handler: function (newValue, oldValue) {\n        console.log(\"newValue: \", newValue);\n        console.log(\"oldValue: \", oldValue);\n      },\n      deep: true\n    }\n  },\n  computed: {\n    formMode() {\n      if (this.productEdit?.ProductsId) {\n        return 1;\n      } else {\n        return 0;\n      }\n    }\n  },\n  data() {\n    return {\n      formInfo: Form,\n      dateConfig: DateConfig,\n      counter: 1,\n      departmentURL: departmentURL,\n      product: {},\n      fixedAsset: {\n        department_code: \"\",\n        department_id: \"\",\n        depreciation_value: 0,\n        depreciation_rate: 1,\n        perchase_date: new Date(Date.now()),\n        used_date: new Date(Date.now()),\n        production_year: new Date().getFullYear(),\n        life_time: 10,\n        quantity: 1\n      },\n      itemSelectCbb: {},\n      isShowPopup: false,\n      isShowNotice: false,\n      msgError: \"\",\n      currentTabindex: 1,\n      listErrorRequired: [],\n      error: {\n        fixed_asset_code: \"\",\n        fixed_asset_name: \"\",\n        department_code: \"\",\n        fixed_asset_category_code: \"\",\n        quantity: \"\",\n        cost: \"\",\n        life_time: \"\",\n        depreciation_rate: \"\",\n        depreciation_value: \"\",\n        tracked_year: \"\"\n      } //  các thông báo lỗi\n    };\n  },\n\n  methods: {\n    /**\r\n     * @description: Đóng form\r\n     * @author: DDT(26/06/2023)\r\n     */\n    handleCloseForm() {\n      this.$emit(\"close-form\");\n    },\n    onSubmit() {\n      axios.post(\"https://64798739a455e257fa6347c2.mockapi.io/users\");\n    },\n    /**\r\n    * @description: format tiền\r\n    * @author: DDT(26/06/2023)\r\n    */\n    formatCost() {\n      this.fixedAsset.product_price = this.formatMoney(this.fixedAsset.product_price);\n      this.fixedAsset.depreciation_value = this.formatMoney(parseInt(this.convertMoneyToNum(this.fixedAsset.product_price) * this.fixedAsset.depreciation_rate) / 10);\n    },\n    /**\r\n     * @description: Chuyển tiền thành số\r\n     * @param {*} data \r\n     * @author: DDTung (04/07/2023)\r\n     */\n    convertMoneyToNum(data) {\n      return MISAFunction.convertMoneyToNum(data);\n    },\n    formatMoney(money) {\n      return MISAFunction.formatMoney(money);\n    },\n    inputEnterNumber(evt) {\n      return MISAFunction.inputEnterNumber(evt);\n    }\n  }\n};","map":{"version":3,"names":["Form","DateConfig","MISAFunction","departmentURL","axios","name","props","isVisable","type","Boolean","productEdit","Object","created","product","formMode","$_MISAEnum","FORM_MODE","ADD","get","then","res","ProductsCode","data","catch","console","log","watch","handler","newValue","oldValue","deep","computed","ProductsId","formInfo","dateConfig","counter","fixedAsset","department_code","department_id","depreciation_value","depreciation_rate","perchase_date","Date","now","used_date","production_year","getFullYear","life_time","quantity","itemSelectCbb","isShowPopup","isShowNotice","msgError","currentTabindex","listErrorRequired","error","fixed_asset_code","fixed_asset_name","fixed_asset_category_code","cost","tracked_year","methods","handleCloseForm","$emit","onSubmit","post","formatCost","product_price","formatMoney","parseInt","convertMoneyToNum","money","inputEnterNumber","evt"],"sources":["C:\\Users\\Admin\\Desktop\\MISA_FRESHER_WEB\\web202305\\mf1661-ddtung\\vuejs\\web-05\\src\\components\\base\\form\\MISAForm.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"isVisable\" id=\"form\">\r\n    <div class=\"form\">\r\n      <div class=\"form-header\">\r\n        <h2>Sửa tài sản</h2>\r\n        <div class=\"icon icon-close\" @click=\"handleCloseForm\"></div>\r\n      </div>\r\n      <div class=\"form-body\">\r\n        <div class=\"row-input\">\r\n          <div class=\"row-input-left\">\r\n            <label for=\"product-code\">{{ formInfo.fixed_asset_code }}\r\n              <span>*</span>\r\n            </label>\r\n            <br />\r\n            <MISAInput v-model=\"product.ProductsCode\" customType=\"text\" customPlaceholder=\"Nhập mã tài sản\"\r\n              customId=\"product-code\" />\r\n          </div>\r\n          <div class=\"row-input-right\">\r\n            <label for=\"product-name\">{{ formInfo.fixed_asset_name }}\r\n              <span>*</span>\r\n            </label>\r\n            <br />\r\n            <MISAInput v-model=\"product.ProductsName\" customType=\"text\" customPlaceholder=\"Nhập tên tài sản\"\r\n              customId=\"product-name\" />\r\n\r\n          </div>\r\n        </div>\r\n        <div class=\"row-input\">\r\n          <div class=\"row-input-left\">\r\n            <label for=\"department-code\">{{ formInfo.department_code }} <span>*</span></label>\r\n            <br />\r\n            <MISACombobox :api=\"departmentURL\" propText=\"DepartmentCode\" propValue=\"DepartmentId\"\r\n              v-model=\"fixedAsset.department_code\" customType=\"text\" customPlaceholder=\"Nhập mã bộ phận sử dụng\"\r\n              customId=\"department-code\" />\r\n            <MISACombobox :api=\"departmentURL\" propText=\"DepartmentCode\" propValue=\"DepartmentId\"\r\n              v-model=\"fixedAsset.department_code\" customType=\"text\" customPlaceholder=\"Nhập mã bộ phận sử dụng\"\r\n              customId=\"department-code\" />\r\n          </div>\r\n          <div class=\"row-input-right\">\r\n            <label for=\"department-name\">{{ formInfo.department_name }}</label>\r\n            <br />\r\n            <MISAInput v-model=\"product.ProductsDepartment\" readonly customType=\"text\" customId=\"department-name\" />\r\n\r\n          </div>\r\n        </div>\r\n        <div class=\"row-input\">\r\n          <div class=\"row-input-left\">\r\n            <label for=\"product-type-code\">{{ formInfo.fixed_asset_category_code }} <span>*</span></label>\r\n            <br />\r\n            <MISACombobox customType=\"text\" customPlaceholder=\"Chọn mã loại tài sản\" customId=\"product-type-code\" />\r\n          </div>\r\n          <div class=\"row-input-right\">\r\n            <label for=\"product-type-name\">{{ formInfo.fixed_asset_category_name }}</label>\r\n            <br />\r\n            <MISAInput readonly customType=\"text\" customId=\"product-type-name\" />\r\n          </div>\r\n        </div>\r\n        <div class=\"row-input-cl3\">\r\n          <div class=\"item-input-cl3\">\r\n            <label for=\"product-quantity\">{{ formInfo.quantity }} <span>*</span></label>\r\n            <br />\r\n            <MISACombobox2 v-model=\"fixedAsset.quantity\" customType=\"text\" customClass=\"no-spinners text-end\"\r\n              customId=\"product-quantity\" />\r\n\r\n          </div>\r\n          <div class=\"item-input-cl3\">\r\n            <label for=\"product-price\">{{ formInfo.cost }} <span>*</span></label>\r\n            <br />\r\n            <MISAInput :formatCost=\"formatCost\" :inputNumber=\"inputEnterNumber\" customType=\"text\"\r\n              v-model=\"fixedAsset.product_price\" customClass=\" text-end\" customId=\"product-price\" />\r\n\r\n          </div>\r\n          <div class=\"item-input-cl3\">\r\n            <label for=\"life-time\">{{ formInfo.life_time }} <span>*</span></label>\r\n            <br />\r\n            <MISAInput customType=\"number\" v-model=\"fixedAsset.life_time\" customClass=\"no-spinners item-input text-end\"\r\n              customId=\"life-time\" />\r\n          </div>\r\n        </div>\r\n        <div class=\"row-input-cl3\">\r\n          <div class=\"item-input-cl3\">\r\n            <label for=\"product-depreciation-rate\">{{ formInfo.depreciation_rate }} <span>*</span></label>\r\n            <br />\r\n            <MISACombobox2 v-model=\"fixedAsset.depreciation_rate\" customClass=\"no-spinners text-end\"\r\n              customId=\"product-depreciation-rate\" customType=\"number\" :min=\"fixedAsset.depreciation_rate\" />\r\n          </div>\r\n          <div class=\"item-input-cl3\">\r\n            <label for=\"product-depreciation-value\">{{ formInfo.depreciation_value }} <span>*</span></label>\r\n            <br />\r\n            <MISAInput :inputNumber=\"inputEnterNumber\" customType=\"text\" v-model=\"fixedAsset.depreciation_value\"\r\n              customClass=\"text-end\" customId=\"product-depreciation-value\" />\r\n\r\n          </div>\r\n\r\n          <div class=\"item-input-cl3\">\r\n            <label for=\"year-tracking\">{{ formInfo.production_year }}</label>\r\n            <br />\r\n            <MISAInput readonly customClass=\"text-end\" id=\"year-tracking\" v-model=\"fixedAsset.production_year\" />\r\n\r\n          </div>\r\n        </div>\r\n        <div class=\"row-input-end\">\r\n          <div class=\"item-input-cl3\">\r\n            <label for=\"purchase-date\">{{ formInfo.purchase_date }}</label>\r\n            <br />\r\n            <ElDatePicker v-model=\"fixedAsset.perchase_date\" type=\"date\" :placeholder=\"dateConfig.Format\"\r\n              :format=\"dateConfig.Format\"></ElDatePicker>\r\n\r\n          </div>\r\n          <div class=\"item-input-cl3\">\r\n            <label for=\"used-date\">{{ formInfo.used_date }}</label>\r\n            <br />\r\n            <ElDatePicker v-model=\"fixedAsset.used_date\" type=\"date\" :placeholder=\"dateConfig.Format\"\r\n              :format=\"dateConfig.Format\"></ElDatePicker>\r\n\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"form-footer\">\r\n        <MISAButton :text=\"formInfo.btnSave\" class=\"btn-save btn--primary text-white\" @click=\"onSubmit\"></MISAButton>\r\n        <MISAButton :text=\"formInfo.btnCancel\" class=\"btn--primary btn-cancel \" @click=\"handleCloseForm\">\r\n        </MISAButton>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Form } from \"../../../js/common/form.js\"\r\nimport { DateConfig } from \"../../../js/common/config.js\";\r\nimport MISAFunction from \"../../../js/common/function\";\r\nimport { departmentURL } from \"@/js/common/urlAsset\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"MISAForm\",\r\n  props: {\r\n    isVisable: {\r\n      type: Boolean\r\n    },\r\n    productEdit: {\r\n      type: Object\r\n    }\r\n  },\r\n  created() {\r\n    this.product = this.productEdit;\r\n    if (this.formMode == this.$_MISAEnum.FORM_MODE.ADD) {\r\n      // Lẫy mã nhân viên mới:\r\n      axios\r\n        .get(\"https://cukcuk.manhnv.net/api/v1/Customers/NewCustomerCode\")\r\n        .then((res) => {\r\n          this.product.ProductsCode = res.data;\r\n        })\r\n        .catch((res) => {\r\n          console.log(); (res);\r\n        });\r\n    }\r\n  },\r\n\r\n  watch: {\r\n    product: {\r\n      handler: function (newValue, oldValue) {\r\n        console.log(\"newValue: \", newValue);\r\n        console.log(\"oldValue: \", oldValue);\r\n      },\r\n      deep: true,\r\n    },\r\n  },\r\n\r\n  computed: {\r\n    formMode() {\r\n      if (this.productEdit?.ProductsId) {\r\n        return 1;\r\n      } else {\r\n        return 0;\r\n      }\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      formInfo: Form,\r\n      dateConfig: DateConfig,\r\n      counter: 1,\r\n      departmentURL: departmentURL,\r\n      product: {},\r\n      fixedAsset: {\r\n        department_code: \"\",\r\n        department_id: \"\",\r\n        depreciation_value: 0,\r\n        depreciation_rate: 1,\r\n        perchase_date: new Date(Date.now()),\r\n        used_date: new Date(Date.now()),\r\n        production_year: new Date().getFullYear(),\r\n        life_time: 10,\r\n        quantity: 1,\r\n\r\n      },\r\n\r\n      itemSelectCbb: {},\r\n      isShowPopup: false,\r\n      isShowNotice: false,\r\n      msgError: \"\",\r\n      currentTabindex: 1,\r\n      listErrorRequired: [],\r\n      error: {\r\n        fixed_asset_code: \"\",\r\n        fixed_asset_name: \"\",\r\n        department_code: \"\",\r\n        fixed_asset_category_code: \"\",\r\n        quantity: \"\",\r\n        cost: \"\",\r\n        life_time: \"\",\r\n        depreciation_rate: \"\",\r\n        depreciation_value: \"\",\r\n        tracked_year: \"\",\r\n      }, //  các thông báo lỗi\r\n\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    /**\r\n     * @description: Đóng form\r\n     * @author: DDT(26/06/2023)\r\n     */\r\n    handleCloseForm() {\r\n      this.$emit(\"close-form\");\r\n    },\r\n    onSubmit() {\r\n      axios.post(\"https://64798739a455e257fa6347c2.mockapi.io/users\",)\r\n    },\r\n    /**\r\n   * @description: format tiền\r\n   * @author: DDT(26/06/2023)\r\n   */\r\n    formatCost() {\r\n      this.fixedAsset.product_price = this.formatMoney(this.fixedAsset.product_price);\r\n      this.fixedAsset.depreciation_value = this.formatMoney(parseInt(\r\n        this.convertMoneyToNum(this.fixedAsset.product_price) *\r\n        this.fixedAsset.depreciation_rate\r\n      ) / 10);\r\n\r\n    },\r\n    /**\r\n     * @description: Chuyển tiền thành số\r\n     * @param {*} data \r\n     * @author: DDTung (04/07/2023)\r\n     */\r\n    convertMoneyToNum(data) {\r\n      return MISAFunction.convertMoneyToNum(data);\r\n    },\r\n    formatMoney(money) {\r\n      return MISAFunction.formatMoney(money);\r\n    },\r\n    inputEnterNumber(evt) {\r\n      return MISAFunction.inputEnterNumber(evt);\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style  >\r\n.form,\r\n.form .cancel {\r\n  background-color: #fff\r\n}\r\n\r\n#form {\r\n  background-color: rgba(0, 0, 0, .2);\r\n  z-index: 2000;\r\n  width: 100%;\r\n  height: 100%;\r\n  margin: auto;\r\n  position: fixed;\r\n  display: flex;\r\n  top: 0;\r\n  left: 0\r\n}\r\n\r\n.form {\r\n  width: fit-content;\r\n  height: fit-content;\r\n  margin: 25px auto;\r\n  border-radius: 5px\r\n}\r\n\r\n.form-footer,\r\n.form-header {\r\n  display: flex;\r\n  height: 52px;\r\n  box-sizing: border-box\r\n}\r\n\r\n.form-header {\r\n  width: 100%;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 16px 16px 0\r\n}\r\n\r\n.form-body {\r\n  padding: 0 16px 16px\r\n}\r\n\r\nlabel span {\r\n  color: red\r\n}\r\n\r\n.row-input {\r\n  display: flex;\r\n  margin: 20px 0\r\n}\r\n\r\n.item-input-cl3 {\r\n  margin-right: 16px;\r\n\r\n}\r\n\r\n.item-input-cl3,\r\n.row-input-left {\r\n  position: relative\r\n}\r\n\r\n.row-input-left input {\r\n  width: 260px;\r\n  margin-right: 0 !important;\r\n  padding: 0 14px !important\r\n}\r\n\r\n.row-input-right input {\r\n  width: 536px;\r\n  padding: 0 14px !important;\r\n  margin-right: 0 !important\r\n}\r\n\r\n.row-input-cl3 input,\r\n.row-input-end input {\r\n  padding: 0 14px !important;\r\n  margin-right: 0 !important;\r\n  width: 260px\r\n}\r\n\r\n#product-quantity,\r\n#product-depreciation-rate {\r\n  padding: 0 36px !important;\r\n  margin-right: 0 !important;\r\n  width: 260px\r\n}\r\n\r\n.form input {\r\n  padding: 0 36px;\r\n  margin-top: 8px;\r\n  border-radius: 4px;\r\n  border: 1px solid #afafaf;\r\n  height: 36px;\r\n  box-sizing: border-box\r\n}\r\n\r\n.row-input-right {\r\n  margin-left: 16px\r\n}\r\n\r\n.form input::placeholder {\r\n  font-style: italic\r\n}\r\n\r\ninput {\r\n  outli